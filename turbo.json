{
	"$schema": "https://turborepo.com/schema.json",
	"tasks": {
		"build": {
			"cache": false
		},
		"dev": {
			"cache": false,
			"dependsOn": ["@org/api-client#build"],
			"persistent": true
		},
		"docker:up": {
			"cache": false
		},
		"openapi:export": {
			"cache": false
		},
		"prisma:generate": {
			"cache": false
		},
		"prisma:migrate": {
			"cache": false
		},
		"setup:local": {
			"cache": false,
			"dependsOn": [
				"@org/api#setup:local:env",
				"@org/client#setup:local:env",
				"@org/api#docker:up",
				"@org/api#prisma:migrate",
				"@org/api#prisma:generate",
				"@org/api#openapi:export",
				"@org/api-client#build"
			]
		},
		"setup:local:env": {
			"cache": false
		}
	}
}
