{
	"$schema": "https://turborepo.com/schema.json",
	"tasks": {
		"build": {
			"dependsOn": ["^build:api-client"],
			"outputs": ["dist/**"]
		},
		"build:api-client": {
			"cache": false
		},
		"dev": {
			"cache": false,
			"dependsOn": ["^build:api-client", "docker:up"],
			"persistent": true
		},
		"docker:up": {
			"cache": false
		},
		"prisma:migrate": {
			"cache": false
		},
		"prisma:generate": {
			"cache": false
		},
		"setup:local:env": {
			"cache": false
		},
		"setup:local": {
			"cache": false,
			"dependsOn": ["setup:local:env", "docker:up", "prisma:migrate", "prisma:generate", "^build:api-client"]
		}
	}
}
